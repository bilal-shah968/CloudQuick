{
  "Logging": {
    "LogLevel": { // All providers, LogLevel applies to all the enabled providers.
      "Default": "Error", // Default logging, Error and higher.
      "Microsoft": "Warning" // All Microsoft* categories, Warning and higher.
    },
    "Console": { // Console provider.
      "LogLevel": {
        "Default": "Error", // Overrides preceding LogLevel:Default setting.
        "Microsoft.Hosting": "Trace" // Console:Microsoft.Hosting category.
      }
    },
    "Debug": { // Debug provider.
      "LogLevel": {
        "Default": "Trace", // Overrides preceding LogLevel:Default setting.
        "Microsoft.Hosting": "Error" // Debug:Microsoft.Hosting category.
      }
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "CloudDBConnection":"Data Source=(localdb)\\MSSQLLocalDB;Initial Catalog=demodb;Integrated Security=True;Encrypt=True;Trust Server Certificate=True"
  }
  

}
